<UserControl x:Class="Arent3d.Architecture.Routing.App.Forms.Dilog2Buttons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Arent3d.Architecture.Routing.App.Forms"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="root">
    <Grid Width="200" Height="30">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Button x:Name="Import"  Content="OK" Width="60" Height="30"
                Grid.Column="0"
                Click="Import_OnClick"/>
        <Button x:Name="Export" Content="Apply" Width="60" Height="30"
                Grid.Column="1"
                Click="Export_OnClick"/>
    </Grid>
    
</UserControl>
