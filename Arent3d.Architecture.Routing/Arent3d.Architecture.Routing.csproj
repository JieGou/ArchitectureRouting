<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <LangVersion>9</LangVersion>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Platforms>x64</Platforms>
    <Authors>Arent Inc.</Authors>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|x64' ">
    <DefineConstants>TRACE;DUMP_LOGS;</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Architecture.Routing.Core">
      <HintPath>..\Libraries\Architecture.Routing.Core.dll</HintPath>
    </Reference>
    <Reference Include="Arent3d.Collision">
      <HintPath>..\Libraries\Arent3d.Collision.dll</HintPath>
    </Reference>
    <Reference Include="Arent3d.Geometry">
      <HintPath>..\Libraries\Arent3d.Geometry.dll</HintPath>
    </Reference>
    <Reference Include="Arent3d.Math">
      <HintPath>..\Libraries\Arent3d.Math.dll</HintPath>
    </Reference>
    <Reference Include="Arent3d.Utility">
      <HintPath>..\Libraries\Arent3d.Utility.dll</HintPath>
    </Reference>
    <Reference Include="RevitAPI">
      <HintPath>C:\Program Files\Autodesk\Revit 2021\RevitAPI.dll</HintPath>
      <Private>false</Private>
    </Reference>
    <Reference Include="WindowsBase" />
  </ItemGroup>

  <ItemGroup>
    <None Update="SharedParameterFile\Arent Shared Parameter.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\SharedParameterFile\RoutingSharedParameters.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\Families\Routing Rack Guide.rfa">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\Families\Routing Pass Point.rfa">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Assets\SharedParameterFile\PassPointSharedParameters.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Arent3d.Revit\Arent3d.Revit.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Assets" />
  </ItemGroup>

</Project>
